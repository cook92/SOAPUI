<settings>
  <!-- 
  If you prefer to relocate your local repository to somewhere other than ~.m2/repository
  then uncomment the <localrepository/> tag below and specify your location.
  <localRepository>c:/relocated/maven/repository</localRepository>
  -->
	  <offline>false</offline>
	  
    

  <profiles>
    <profile>

      <id>Smartbear-Profile</id>

      <pluginRepositories>

        <pluginRepository>

          <id>smartbear-plugin-repository</id>

          <url>https://www.soapui.org/repository/maven2/</url>

        </pluginRepository>

      </pluginRepositories>

    </profile> 

    <profile>

      <id>Maven-Central-Profile</id>

      <pluginRepositories>

        <pluginRepository>

          <id>maven-repo-central</id>

          <url>https://repo.maven.apache.org/maven2/</url>

        </pluginRepository>

      </pluginRepositories>

    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>Maven-Central-Profile</activeProfile>
 <activeProfile>Smartbear-Profile</activeProfile>
  </activeProfiles>

</settings>
